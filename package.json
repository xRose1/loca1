{
  "name": "loca",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run buildprod && NODE_ENV=production node server.js",
    "buildprod": "NODE_ENV=production node scripts/build.js",
    "dev": "npm-run-all builddev watch",
    "watch": "npm-run-all --parallel watch:img watch:less watch:front:js watch:back:js",
    "watch:front": "npm-run-all --parallel watch:img watch:less watch:front:js",
    "watch:back:js": "nodemon -e js,ejs -w backend -w server.js --inspect=0.0.0.0:9229 server.js",
    "watch:front:js": "nodemon -e js,ejs -w frontend/js -w backend/pages scripts/build.js js",
    "watch:less": "nodemon -e less -w frontend/less scripts/build.js css",
    "watch:img": "nodemon -e png,jpg -w frontend/images scripts/build.js img",
    "builddev": "node scripts/build.js static",
    "lint": "eslint .",
    "test": "LOCA_DEMOMODE=false LOCA_PRODUCTIVE=true mocha",
    "coverage": "LOCA_DEMOMODE=false LOCA_PRODUCTIVE=true nyc --reporter=lcov --reporter=text-summary mocha",
    "mongodump": "node scripts/mongodump.js",
    "mongorestore": "node scripts/mongorestore.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "bootbox": "5.1.3",
    "bootstrap": "3.4.1",
    "bootstrap-datepicker": "1.8.0",
    "ejs": "2.6.1",
    "errorhandler": "1.5.0",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "express-winston": "2.6.0",
    "frontexpress": "1.2.1",
    "handlebars": "4.3.0",
    "i18next": "14.0.1",
    "i18next-browser-languagedetector": "2.2.4",
    "i18next-express-middleware": "1.7.1",
    "i18next-localstorage-cache": "1.1.1",
    "i18next-node-fs-backend": "2.1.3",
    "i18next-sprintf-postprocessor": "0.2.2",
    "i18next-xhr-backend": "1.5.1",
    "intl": "1.2.5",
    "jquery": "3.4.1",
    "jquery-validation": "1.19.0",
    "mathjs": "3.20.2",
    "method-override": "3.0.0",
    "minivents": "2.2.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.23",
    "mongobackup": "0.3.5",
    "mongojs": "2.6.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "serve-favicon": "2.5.0",
    "sugar": "2.0.6",
    "winston": "2.4.4"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "babel-eslint": "10.0.1",
    "browser-sync": "2.26.5",
    "eslint": "5.12.1",
    "fs-extra": "7.0.1",
    "imagemin": "6.1.0",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "6.0.0",
    "less": "3.9.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.9",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "proxyquire": "1.8.0",
    "purify-css": "1.2.5",
    "rollup": "1.1.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-includepaths": "0.2.3",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-terser": "4.0.3",
    "sinon": "7.2.3",
    "supertest": "3.4.2"
  }
}
