<div id="rents" class="list">
    <div data-id="pay-rent-form" class="js-sheet sheet card">
        <%- include paymentform.ejs %>
    </div>

    <div data-id="rents-history" class="js-sheet sheet card">
        <%- include rentshistory.ejs %>
    </div>

    <div data-id="print-doc-selector" class="js-sheet sheet">
        <div class="list-group card">
            <a id="rentcall" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-file-text-o"></i> <%= t('Rent notice') %></h4>
                <p class="list-group-item-text"><%= t('Notice letter for rent payment') %></p>
            </a>
            <a id="recovery1" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-bell-o"></i> <%= t('Late rent - First reminder') %></h4>
                <p class="list-group-item-text"><%= t('Friendly reminder for rent payment') %></p>
            </a>
            <a id="recovery2" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-bullhorn"></i> <%= t('Late rent - Second reminder') %></h4>
                <p class="list-group-item-text"><%= t('Unfriendly reminder for rent payment') %></p>
            </a>
            <a id="recovery3" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-envelope-o"></i> <%= t('Late rent - Last reminder') %></h4>
                <p class="list-group-item-text"><%= t('Order to pay or vacate') %></p>
            </a>
            <a id="paymentorder" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-gavel"></i> <%= t('Recovery of unpaid rents') %></h4>
                <p class="list-group-item-text"><%= t('Request rent recovery by a third party (company, justice)') %></p>
            </a>
            <a id="printinvoices" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-file-text-o"></i> <%= t('Rental invoice') %></h4>
                <p class="list-group-item-text"><%= t('Invoice stating the payment of the rent') %></p>
            </a>
        </div>
    </div>

    <div data-id="email-doc-selector" class="js-sheet sheet">
        <div class="list-group card">
            <a id="emailrentcall" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-envelope-o"></i> <%= t('Rent notice') %></h4>
                <p class="list-group-item-text"><%= t('Notice letter for rent payment') %></p>
            </a>
            <a id="emailrentcallreminder" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-envelope-o"></i> <%= t('Rent notice reminder') %></h4>
                <p class="list-group-item-text"><%= t('Friendly reminder for rent payment') %></p>
            </a>
            <a id="emaillastrentcallreminder" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-envelope-o"></i> <%= t('Last rent notice reminder') %></h4>
                <p class="list-group-item-text"><%= t('Unfriendly reminder for rent payment') %></p>
            </a>
            <a id="emailinvoice" href="#" class="list-group-item">
                <h4 class="list-group-item-heading"><i class="fa fa-envelope-o"></i> <%= t('Rental invoice') %></h4>
                <p class="list-group-item-text"><%= t('Invoice stating the payment of the rent') %></p>
            </a>
        </div>
    </div>

    <div class="js-list-content"></div>
</div>
